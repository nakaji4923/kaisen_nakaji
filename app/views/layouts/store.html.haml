!!! 5
%html
  %head
    %meta{ "http-qeuiv" => "Content-Type", :content => "text/html: charset=utf8" }
    %title 買鮮カタログ
    != stylesheet_link_tag 'reset', 'text', '960', 'store'
    != javascript_include_tag :defaults
    != csrf_meta_tag
  %body#store.container_12
    %div#header.grid_12
      %h1 買鮮カタログ
    .clear
    #colmuns

      #main.grid_5.suffix_2
        - if flash[:notice]
          #notice= flash[:notice]
        - if flash[:alert]
          #alert= flash[:alert]
        != yield
      #side.grid_5
        #cart
          = render :partial => 'cart', :object => @cart
        %ul#nav
          %li
            = link_to "トップ", store_path
          %li
            %a{:href => "#"}ニュース
          %li
            %a{:href => "#"}お問合わせ
          %li
            %a{:href => "#"}ご利用ガイド
      .clear
    #footer.grid_12
      %p Copyright (C) 2010 Enishi Tech Inc. AllRights Reserved.
    .clear
